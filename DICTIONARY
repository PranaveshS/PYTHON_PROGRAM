#32
#SORT DICTIONARIES BY KEY OR VALUE
d = {'one': 1, 'three': 3, 'five': 5, 'two': 2, 'four': 4}
a = sorted(d.items(), key=lambda x: x[1])
print(a)

#33
#DICTIONARY WITH KEYS HAVING MULTIPLE INPUTS
import random as rn
dict = {}
x, y, z = 10, 20, 30
dict[x, y, z] = x + y - z;
x, y, z = 5, 2, 4
dict[x, y, z] = x + y - z;
print(dict)
